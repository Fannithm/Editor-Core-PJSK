{
	"include": [
		"src"
	],
	"compilerOptions": {
		"moduleResolution": "node",
		"target": "ES2021",
		"allowSyntheticDefaultImports": true
	},
	"typedocOptions": {
		"entryPoints": [
			"src/index.ts"
		],
		"name": "Fannithm Editor Core Documentation",
		"includeVersion": true,
		"excludePrivate": true,
		"excludeProtected": true,
		"out": "dist/docs"
	}
}
